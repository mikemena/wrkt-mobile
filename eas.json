{
  "cli": {
    "version": ">= 12.6.2",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "buildConfiguration": "Debug"
      },
      "env": {
        "ENVFILE": ".env.development",
        "ENV": "development",
        "API_URL": "https://wrkt-backend-development.up.railway.app"
      }
    },
    "testflight": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "buildConfiguration": "Release"
      },
      "env": {
        "ENVFILE": ".env.development",
        "ENV": "development",
        "API_URL": "${process.env.API_URL}"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "buildConfiguration": "Release"
      },
      "env": {
        "ENVFILE": ".env.production",
        "ENV": "production",
        "API_URL": "https://api.wrkt.fitness"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
