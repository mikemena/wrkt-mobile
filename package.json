{
  "name": "wrkt",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start:backend": "cd ../wrkt-backend && npm run start",
    "start:expo": "EXPO_PACKAGER_HOSTNAME=localhost ENVFILE=.env.development expo start --dev-client",
    "log:ios": "npx react-native log-ios",
    "start:dev": "ENVFILE=.env.development expo start --dev-client",
    "start:prod": "ENVFILE=.env.production expo start --dev-client",
    "ios:dev": "ENVFILE=.env.development expo start --ios",
    "ios:prod": "ENVFILE=.env.production expo start --ios",
    "ios:pro:dev": "ENVFILE=.env.development expo start --ios --simulator='iPhone 15 Pro'",
    "ios:pro:prod": "ENVFILE=.env.production expo start --ios --simulator='iPhone 15 Pro'",
    "android:dev": "ENVFILE=.env.development expo run:android",
    "android:prod": "ENVFILE=.env.production expo run:android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "build:ios:dev": "ENVFILE=.env.development npx expo prebuild -p ios && cd ios && pod install && cd .. && xcodebuild -workspace ios/WRKT.xcworkspace -scheme WRKT -configuration Release -destination 'generic/platform=iOS'",
    "build:ios:prod": "ENVFILE=.env.production npx expo prebuild -p ios && xcodebuild -workspace ios/WRKT.xcworkspace -scheme WRKT -configuration Release -destination 'generic/platform=iOS'",
    "release:ios:dev": "ENVFILE=.env.development npx expo prebuild -p ios && xcodebuild -workspace ios/WRKT.xcworkspace -scheme WRKT -configuration Release -destination 'generic/platform=iOS' && xcrun altool --upload-app --type ios --file /Users/mike/Library/Developer/Xcode/DerivedData/WRKT-fymmwfwencvowggeyvwzzzceqlqo/Build/Products/Release-iphoneos/WRKT.ipa --username \"$APPLE_ID\" --password \"$ASC_APP_SPECIFIC_PASSWORD\"",
    "build:ios": "expo build:ios",
    "clear-cache": "expo start --clear",
    "start": "expo start --dev-client"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@invertase/react-native-apple-authentication": "^2.4.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-picker/picker": "2.7.5",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "anser": "^2.1.1",
    "expo": "~51.0.38",
    "expo-constants": "~16.0.2",
    "expo-crypto": "~13.0.2",
    "expo-dev-client": "~4.0.29",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.10",
    "expo-haptics": "^13.0.1",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-splash-screen": "^0.27.7",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "jsonwebtoken": "^9.0.2",
    "memoize-one": "^6.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.10",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/metro-config": "^0.19.9",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "dotenv": "^16.4.7",
    "metro-config": "^0.81.0",
    "react-test-renderer": "18.2.0"
  },
  "private": true
}
